<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/numboard_whole_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@android:color/white"
    android:tag="0x7f0700a7">

    <EditText
        android:id="@+id/numboard_pwd_edittext"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_centerInParent="true"
        android:editable="false"
        android:enabled="false"
        android:ellipsize="end"
        android:gravity="left|center"
        android:inputType="numberPassword"
        android:maxLength="16"
        android:maxLines="1"
        android:minHeight="35.0dp"
        android:shadowColor="#fff6dbe0"
        android:shadowDx="-1.0"
        android:shadowDy="-1.0"
        android:shadowRadius="1.0"
        android:singleLine="true"
        android:tag="0x7f0700a3"
        android:textColor="@android:color/black"
        android:textColorHint="#77ffffff"
        android:textSize="16.0sp" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/numboard_pwd_edittext"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="60dp"
        android:text="@string/enter_pwd"
        android:textSize="18sp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_below="@id/numboard_pwd_edittext">

        <LinearLayout
            android:id="@+id/ll_dialer_line1"
            style="@style/style_ll_dialer">

            <Button
                android:id="@+id/btn_1"
                style="@style/style_btn_dialer"
                android:text="1" />

            <Button
                android:id="@+id/btn_2"
                style="@style/style_btn_dialer"
                android:text="2" />
            <Button
                android:id="@+id/btn_3"
                style="@style/style_btn_dialer"
                android:text="3" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_dialer_line2"
            style="@style/style_ll_dialer"
            android:layout_below="@id/ll_dialer_line1">

            <Button
                android:id="@+id/btn_4"
                style="@style/style_btn_dialer"
                android:text="4" />

            <Button
                android:id="@+id/btn_5"
                style="@style/style_btn_dialer"
                android:text="5" />
            <Button
                android:id="@+id/btn_6"
                style="@style/style_btn_dialer"
                android:text="6" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_dialer_line3"
            style="@style/style_ll_dialer"
            android:layout_below="@id/ll_dialer_line2">

            <Button
                android:id="@+id/btn_7"
                style="@style/style_btn_dialer"
                android:text="7" />

            <Button
                android:id="@+id/btn_8"
                style="@style/style_btn_dialer"
                android:text="8" />
            <Button
                android:id="@+id/btn_9"
                style="@style/style_btn_dialer"
                android:text="9" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_dialer_line4"
            style="@style/style_ll_dialer"
            android:layout_below="@id/ll_dialer_line3">

            <Button
                android:id="@+id/btn_cancel"
                style="@style/style_btn_dialer"
                android:text="EXIT" />

            <Button
                android:id="@+id/btn_0"
                style="@style/style_btn_dialer"
                android:text="0" />
            <Button
                android:id="@+id/btn_del"
                style="@style/style_btn_dialer"
                android:text="DEL" />
        </LinearLayout>
    </RelativeLayout>
